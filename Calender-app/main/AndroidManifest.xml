<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.t3421.calenderapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddEvent" />
        <activity android:name=".EventsDb" />
        <activity android:name=".EventDb" />
        <activity android:name=".AddCalenderDate" />
        <activity android:name=".AddTime" />
        <activity android:name=".WeekView" />
        <activity android:name=".AgendaView"></activity>
    </application>

</manifest>